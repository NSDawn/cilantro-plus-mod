	"entrypoints": {
		"main": [
			"dev.nsdawn.cilantromod.CilantroMod"
		],
		"client": [
			"dev.nsdawn.cilantromod.CilantroModClient"
		],
		"fabric-datagen": [
			"dev.nsdawn.cilantromod.CilantroModDataGenerator"
		]
	},
	"mixins": [
		"cilantro-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
