{
	"schemaVersion": 1,
	"id": "cilantro-mod",
	"version": "${version}",
	"name": "Cilantro Mod",
	"description": "A Minecraft mod that adds Cilantro to the game, so I can learn how to use Fabric to make mods :)",
	"authors": [
		"nsdawn"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/cilantro-mod/icon.png",
